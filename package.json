{
  "name": "i18n-manager-tauri",
  "private": true,
  "version": "0.2.3",
  "scripts": {
    "next:dev": "next dev -p 1420",
    "next:build": "next build && next export -o dist",
    "ci:tauri:before-build": "if [[ ! -d \"./dist\" ]]; then echo \"Next.js export does not exists at ./dist\" && exit 1; else echo \"Next.js already built\"; fi",
    "tauri:before-build": "if test \"$CI\" = true; then [ ! -d \"./dist\" ] && yarn next:build || echo \"dist already exists, skipping build\"; else yarn next:build; fi",
    "tauri:setup-config": "node ./scripts/setup-config.js",
    "build": "yarn tauri:setup-config && tauri build --config src-tauri/tauri.conf.prod.json",
    "tauri": "tauri",
    "dev": "tauri dev",
    "bump": "./scripts/bump-version.sh",
    "release": "./scripts/sync-release-branch.sh",
    "prepare": "if [[ -z \"$CI\" ]]; then husky install; else echo \"Skipping husky in CI\"; fi",
    "postinstall": "cd ./node_modules/jotai-cache/ && [ ! -d \"./dist\" ] && yarn && yarn compile && rm -rf ./node_modules || echo \"jotai-cache already compiled\""
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.2",
    "@tauri-apps/api": "^1.2.0",
    "axios": "^1.2.2",
    "clsx": "^1.2.1",
    "dot-prop": "6",
    "flat": "^5.0.2",
    "fuse.js": "^6.6.2",
    "jotai": "^1.12.0",
    "jotai-cache": "github:jotai-labs/jotai-cache#jotai-new-async-api",
    "lodash.uniqby": "^4.7.0",
    "minisearch": "^6.0.0",
    "next": "13",
    "p-memoize": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.2",
    "react-use": "^17.4.0",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-vtree": "^3.0.0-beta.3",
    "react-window": "^1.8.8",
    "sort-on": "^5.2.0",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#9bd993a",
    "traverse": "^0.6.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.2",
    "@types/flat": "^5.0.2",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/traverse": "^0.6.32",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.2",
    "postcss": "^8.4.20",
    "semver": "^7.3.8",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "browserslist": [
    "Safari 13"
  ]
}
